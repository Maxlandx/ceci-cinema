<form method="post" action="{{ path('seance_delete', {'dateSeance': seance.dateSeance}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ seance.dateSeance) }}">
    <button class="btn">Delete</button>
</form>
